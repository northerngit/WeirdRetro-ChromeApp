{
  "name": "Weird Retro",
  "description": "A digital cabinet of curiosities. A retro repository of ethereal ephemera.",
  "version": "1.3",
  "manifest_version": 2,
  "minimum_chrome_version": "37.0.0.0",
  "permissions": ["webview", "storage", "pointerLock", "system.display" ],
  "app": {
    "background": {
      "scripts": ["js/foam.js", "js/cab.js", "config.js", "background_main.js"]
    }
  },
  "kiosk_enabled": true,
  "default_locale": "en",
  "icons": {
    "128": "img/128.png"
  }
}
